import java.util.Calendar;

public class EX10_1 {

	public static void main(String[] args) {

		Calendar today = Calendar.getInstance();
		System.out.println("몇 년도?"+ today.get(Calendar.YEAR));
		System.out.println("몇월인지?"+(today.get(Calendar.MONTH)+1)+"월");
		System.out.println("올해의 몇주차?"+today.get(Calendar.WEEK_OF_YEAR));
		System.out.println("이달의 몇째주?"+today.get(Calendar.DAY_OF_WEEK));
	}

}
